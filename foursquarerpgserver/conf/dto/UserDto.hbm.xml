<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="dtos.UserDto" table="user">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="username" type="string">
			<column name="username" not-null="true" />
		</property>
		<property name="token" type="string">
			<column name="token" not-null="true" />
		</property>
		<property name="hp" type="java.lang.Integer">
			<column name="hp" not-null="true" />
		</property>
		<property name="attack" type="java.lang.Integer">
			<column name="attack" not-null="true" />
		</property>
		<property name="defense" type="java.lang.Integer">
			<column name="defense" not-null="true" />
		</property>
		<property name="stamina" type="java.lang.Integer">
			<column name="stamina" not-null="true" />
		</property>
		<property name="experience" type="java.lang.Integer">
			<column name="exp" not-null="true" />
		</property>
		<property name="gold" type="java.lang.Integer">
			<column name="gold" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
